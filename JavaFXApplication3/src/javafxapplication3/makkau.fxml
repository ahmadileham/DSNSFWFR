<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="478.0" prefWidth="694.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication3.Controller">
   <children>
   
               <Label fx:id="welcomeText" layoutX="14.0" text="CONFESSION PAGE" textAlignment="CENTER">
         <font>
            <Font name="Roboto Bold" size="44.0" />
         </font>
      </Label>
      <Button fx:id="searchButton" layoutX="566.0" layoutY="157.0" mnemonicParsing="false" onAction="#searchScene" text="Search" />
      <Button fx:id="adminButton" layoutX="567.0" layoutY="210.0" mnemonicParsing="false" onAction="#adminLoginScene" text="Admin" />
      <TextArea id="confessionPrint" fx:id="confessionPrint" editable="false" layoutX="14.0" layoutY="54.0" prefHeight="342.0" prefWidth="486.0" text="&#10;" />
      <Button fx:id="replyButton" layoutX="567.0" layoutY="105.0" mnemonicParsing="false" onAction="#replyScene" text="Reply" />
      <Button fx:id="uploadButton" layoutX="532.0" layoutY="54.0" mnemonicParsing="false" onAction="#submitScene" text="Upload confession" />
      <HBox layoutX="14.0" layoutY="404.0" prefHeight="60.0" prefWidth="486.0">
         <children>
            <Button onAction="#onBackButtonClick" prefHeight="60.0" prefWidth="247.0" text="Back">
               <HBox.margin>
                  <Insets />
               </HBox.margin></Button>
            <Button onAction="#onNextButtonClick" prefHeight="60.0" prefWidth="238.0" text="Next">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Button fx:id="replyScene" layoutX="567.0" layoutY="260.0" mnemonicParsing="false" onAction="#readScene" text="Read" />
      <Button fx:id="viewRepliesButton" layoutX="549.0" layoutY="306.0" mnemonicParsing="false" onAction="#viewRepliesScene" text="View Replies" />
   </children>
</AnchorPane>
