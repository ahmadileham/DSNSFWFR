<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication3.Controller4">
   <children>
      <ChoiceBox fx:id="choicesBox" layoutX="53.0" layoutY="122.0" prefWidth="150.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </ChoiceBox>
      <Label layoutX="46.0" layoutY="42.0" text="SEARCH ANJING">
         <font>
            <Font name="Comic Sans MS Bold" size="38.0" />
         </font>
      </Label>
      <TextField fx:id="searchText" layoutX="53.0" layoutY="164.0" />
      <TextArea fx:id="searchPrint" editable="false" layoutX="226.0" layoutY="122.0" prefHeight="200.0" prefWidth="357.0" />
      <Button fx:id="searchButton" layoutX="152.0" layoutY="297.0" mnemonicParsing="false" onAction="#searchFunction" text="Search" />
      <Button fx:id="backButton" layoutX="53.0" layoutY="297.0" mnemonicParsing="false" onAction="#mainScene" text="Home" />
      <Button fx:id="searchButton1" layoutX="534.0" layoutY="346.0" mnemonicParsing="false" onAction="#onNextButtonClick" text="Next" />
      <Button fx:id="backButton1" layoutX="226.0" layoutY="346.0" mnemonicParsing="false" onAction="#onBackButtonClick" text="Previous" />
   </children>
</AnchorPane>
